<!doctype html>
<html class="no-js" lang="{{ request.locale.iso_code }}">

<head>

   <script src="https://www.googleoptimize.com/optimize.js?id=OPT-T8JWF84"></script>

   {% if template == 'product.logo' %}
  <!-- Infinite Options by ShopPad -->
  <script>
    window.Shoppad = window.Shoppad || {},
      window.Shoppad.apps = window.Shoppad.apps || {},
      window.Shoppad.apps.infiniteoptions = window.Shoppad.apps.infiniteoptions || {},
      window.Shoppad.apps.infiniteoptions.submitPriceModCartAnyways = true;
  </script>
   <script
      src="//d1liekpayvooaz.cloudfront.net/apps/customizery/customizery.js?shop={{shop.permanent_domain}}"></script>
   {% endif %}

   <meta name="google-site-verification" content="Q8Pkw4rw4A4S7Ijjy5jcwHWM02s0qYekkN-WotrpFJA" />
   <meta charset="utf-8">
   <meta http-equiv="X-UA-Compatible" content="IE=edge">
   <meta name="viewport" content="width=device-width,initial-scale=1">
   <meta name="theme-color" content="">

